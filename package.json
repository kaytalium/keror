{
  "name": "keror",
  "version": "1.0.0",
  "description": "Simple desktop application that allow users to see into the future.",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"electron .\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "build": "electron-packager . --overwrite --out=src  --version-string.ProductName=\"mg2 Electron Angular app\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaytalium/keror.git"
  },
  "author": "Ovel Heslop <kayheslop@gmail.com> (http://www.knymbus.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaytalium/keror/issues"
  },
  "homepage": "https://github.com/kaytalium/keror#readme",
  "dependencies": {
    "concurrently": "3.5.1",
    "electron": "1.7.9",
    "electron-packager": "10.1.0"
  },
  "devDependencies": {
    "electron-reload": "1.2.2"
  }
}
